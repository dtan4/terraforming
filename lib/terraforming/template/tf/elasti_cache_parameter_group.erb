<% elasti_cache_parameter_groups.each do |parameter_group| -%>
resource "aws_elasticache_parameter_group" "<%= module_name_of(parameter_group) %>" {
    name        = "<%= parameter_group.cache_parameter_group_name %>"
    family      = "<%= parameter_group.cache_parameter_group_family %>"
    description = "<%= parameter_group.description %>"

<% elasti_cache_parameters_in(parameter_group).each do |parameter| -%>
    parameter {
        name  = "<%= parameter.parameter_name %>"
        value = "<%= parameter.parameter_value || '' %>"
    }

<% end -%>
}

<% end -%>
