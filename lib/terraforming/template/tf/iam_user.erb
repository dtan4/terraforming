<% iam_users.each do |user| -%>
resource "aws_iam_user" "<%= module_name_of(user) %>" {
    name = "<%= user.user_name %>"
    path = "<%= user.path %>"
<% if user[:tags].length > 0 -%>

    tags = {
<% user[:tags].each do |tag| -%>
        "<%= tag.key %>" = "<%= tag.value %>"
<% end -%>
    }
<% end -%>
}

<% end -%>
