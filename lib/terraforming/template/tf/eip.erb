<% eips.each do |addr| -%>
resource "aws_eip" "<%= module_name_of(addr) %>" {
<% if addr.instance_id -%>
    instance          = "<%= addr.instance_id %>"
<% elsif addr.network_interface_id -%>
    network_interface = "<%= addr.network_interface_id %>"
<% end -%>
    vpc               = <%= vpc?(addr) %>
}

<% end -%>
