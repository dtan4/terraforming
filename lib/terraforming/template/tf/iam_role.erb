<% iam_roles.each do |role| -%>
resource "aws_iam_role" "<%= module_name_of(role) %>" {
    name               = "<%= role.role_name %>"
    path               = "<%= role.path %>"
    assume_role_policy = <<POLICY
<%= prettify_policy(role.assume_role_policy_document, unescape: true) %>
POLICY
}

<% end -%>
